generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id     Int    @id @default(autoincrement())
  amount String
  userId Int?
  User   User?  @relation(fields: [userId], references: [id])
}

model User {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  name        String?
  Transaction Transaction[]
}
